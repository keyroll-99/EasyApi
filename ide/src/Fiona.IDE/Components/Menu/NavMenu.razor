<Bar
    Mode="BarMode.VerticalInline"
    CollapseMode="BarCollapseMode.Small"
    Breakpoint="Breakpoint.Desktop"
    NavigationBreakpoint="Breakpoint.Tablet"
    ThemeContrast="ThemeContrast.Dark">
    <BarBrand>
        <BarItem>
            <div class="bar-brand-wrapper">
                <div class="logo-wrapper">
                    <Image Source="images/logo.jpg" Text="logo" Fluid/>
                </div>
                Fiona
            </div>
        </BarItem>
    </BarBrand>
    <BarMenu>
        <BarStart>
            <BarItem>
                <BarDropdown>
                    <BarDropdownToggle>
                        Project
                    </BarDropdownToggle>
                    <BarDropdownMenu>
                        <BarDropdownItem>New project </BarDropdownItem>
                    </BarDropdownMenu>
                </BarDropdown>

            </BarItem>
            <BarItem>
                <BarLink To="/app-builder">app builder</BarLink>
            </BarItem>
        </BarStart>
    </BarMenu>
</Bar>

@code
{
    [Inject]
    private MenuService MenuService { get; init; } = default!;

}